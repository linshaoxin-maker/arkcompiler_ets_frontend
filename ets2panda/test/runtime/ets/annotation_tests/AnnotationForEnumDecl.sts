@interface Anno {
   name: string = ""
   id: int = 1
}
@interface Anno2 {}

@Anno
@Anno2
enum E0 { A, B }

@Anno()
@Anno2
enum E1 { A, B }

@Anno({name : "2"})
enum E2 { A, B }

@Anno({id : 1})
enum E3 { A, B }

@Anno({name : "2", id : 1})
enum E4 { A, B }

function main(): void {
    let x0 = E0.A
    let x1 = E1.A
    let x2 = E2.A
    let x3 = E3.A
    let x4 = E4.A
}
