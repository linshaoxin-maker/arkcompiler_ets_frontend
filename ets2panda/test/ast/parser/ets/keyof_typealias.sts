class A{
    field1:int;
    field2:int;
    method1(){};
}

interface B{
    field1:int;
    field2:int;
    method1(){};
}

function main():void{
    type keyofA = keyof A;
    let x1:keyofA = "field1";
    let x2:keyofA = "field2";
    let x3:keyofA = "method1";
    let x4:keyofA = "other field";
    let x5:keyofA = "other method";
    let x6:keyofA = 100;

    type keyofB = keyof B;
    let x7:keyofB = "field1";
    let x8:keyofB = "field2";
    let x9:keyofB = "method1";
    let x10:keyofB = "other field";
    let x11:keyofB = "other method";
    let x12:keyofB = 100;
}

