// Counterexample: Conditional type selection
type Diff<T, U> = T extends U ? never : T; 
type Filter<T, U> = T extends U ? T : never;

type Distribute<T> = T extends infer U ? Filter<T, U> : never;

type MyUnion = string | number | boolean;
type MyType = Distribute<MyUnion>;


