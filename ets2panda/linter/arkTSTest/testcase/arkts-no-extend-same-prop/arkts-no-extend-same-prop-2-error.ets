// Description: An interface inherits from two interfaces with the same method

interface M {
  speed: number
}

interface S {
  frequency: number
}

interface MS {
  speed: number
  frequency: number
}


interface Mover {
  getStatus(): M
}
interface Shaker {
  getStatus(): S
}

interface MoverShaker extends Mover, Shaker {
  getStatus(): MS
}

