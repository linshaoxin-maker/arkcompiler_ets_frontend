// Positive example: Pass arguments directly to the function.
interface Cat {
  name: string
  age: number
}

class CatHandler {
  fn(obj: Cat): void {
    console.log(obj.name, obj.age)
  }
}

const catHandler = new CatHandler()
catHandler.fn({ name: 'Persian cat', age: 11 })
